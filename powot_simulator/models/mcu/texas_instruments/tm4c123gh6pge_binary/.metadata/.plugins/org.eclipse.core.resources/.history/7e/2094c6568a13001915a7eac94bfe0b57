double get_instr_energy(instruction_desc_t *iut){
	int result;
	unsigned long i;

	for(i = 0; i < NUM_OF_INSTRUCTIONS; i++){
		result = strcmp(iut->instr_mnemonic, tm4c_isa[i]->instr_mnemonic);
		if(!result){

		}

	}

	return 0;
}
